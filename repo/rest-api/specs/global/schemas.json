{
	"Item": {
		"type": "object",
		"properties": {
			"id": {
				"type": "string"
			},
			"type": {
				"type": "string"
			},
			"labels": {
				"$ref": "#/Labels"
			},
			"descriptions": {
				"$ref": "#/Descriptions"
			},
			"aliases": {
				"$ref": "#/Aliases"
			},
			"sitelinks": {
				"$ref": "./schema-parts.json#/Sitelinks"
			},
			"statements": {
				"$ref": "./schema-parts.json#/Statements"
			}
		},
		"required": [
			"id",
			"type",
			"labels",
			"descriptions",
			"aliases",
			"statements",
			"sitelinks"
		],
		"discriminator": {
			"propertyName": "type"
		}
	},
	"Labels": {
		"type": "object",
		"additionalProperties": {
			"type": "string"
		},
		"example": {
			"en": "Douglas Adams",
			"fr": "Douglas Adams"
		}
	},
	"Descriptions": {
		"type": "object",
		"additionalProperties": {
			"type": "string"
		},
		"example": {
			"en": "Science-fiction writer",
			"fr": "écrivain de science-fiction"
		}
	},
	"Aliases": {
		"type": "object",
		"additionalProperties": {
			"type": "array",
			"items": { "type": "string" }
		},
		"example": {
			"en": [
				"Douglas Noel Adams",
				"Douglas Noël Adams"
			],
			"fr": [
				"Douglas Noel Adams"
			]
		}
	},
	"Sitelink": {
		"type": "object",
		"properties": {
			"site": {
				"type": "string"
			},
			"title": {
				"type": "string"
			},
			"badges": {
				"type": "array",
				"items": { "type": "string" }
			}
		}
	},
	"Statement": {
		"type": "object",
		"properties": {
			"id": {
				"type": "string",
				"readOnly": true
			},
			"mainsnak": {
				"$ref": "#/Snak"
			},
			"rank": {
				"type": "string"
			},
			"qualifiers": {
				"$ref": "./schema-parts.json#/Snaks"
			},
			"qualifiers-order": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"references": {
				"type": "array",
				"items": { "$ref": "#/Reference" }
			}
		},
		"required": [ "mainsnak" ]
	},
	"Reference": {
		"type": "object",
		"properties": {
			"hash": {
				"description": "Hash of a reference",
				"$ref": "./schema-parts.json#/Hash"
			},
			"snaks": {
				"$ref": "./schema-parts.json#/Snaks"
			},
			"snaks-order": {
				"type": "array",
				"items": {
					"type": "string"
				}
			}
		}
	},
	"Snak": {
		"type": "object",
		"properties": {
			"snaktype": {
				"type": "string"
			},
			"property": {
				"type": "string"
			},
			"hash": {
				"description": "Hash of a snak",
				"$ref": "./schema-parts.json#/Hash"
			},
			"datatype": {
				"type": "string",
				"readOnly": true
			},
			"datavalue": {
				"type": "object",
				"additionalProperties": true,
				"example": {
					"type": "string",
					"value": "I am a goat"
				}
			}
		}
	}
}
